{
  double A,R,pi; //declarando variaveis
  //entrada de dados
  cin>> R;
 pi=3.14159;
  
  
  //calculo
  A=pi*R*R;
  //setar casas decimais 
cout<< fixed<< setprecision(3);
  //saida de dados
  cout<<"AREA = "<< A;
  
  
  
  
  
    
