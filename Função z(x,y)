#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
	double x, y, z;
//entrada de dados
	cin >> x >> y;
//condiÃ§oes
	if (x - y >= 0 && x + y > 0)
	{
		z = sqrt(x - y) / sqrt(x + y);
		cout << " Z = " << z << endl;
	}
	else
		cout << "calculo impossivel";

	return 0;
}
